package youtube.gustavo_guanabara.poo05;

public class Banco {
	
	private String numConta;
	private String tipo; // cc conta corrente / cp conta polpança
	private String dono;
	private double saldo;
	private boolean status;
	
	// conta corrente já ganha 50 reais na conta
	
	abrirConta();
	fecharConta();
	depositar();
	sacar();
	pagarMensal();
	
}
