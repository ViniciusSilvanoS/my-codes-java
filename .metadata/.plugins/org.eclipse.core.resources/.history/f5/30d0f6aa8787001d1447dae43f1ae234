package oo.composicao_teste2.meu.desafio;

public class Produto {

	private String nome;
	private double preco;
	
	
	public Produto(String nome, double preco) {
		super();
		this.nome = nome;
		this.preco = preco;
	}


	public Produto(String nome, double preco) {
		super();
		this.nome = nome;
		this.preco = preco;
	}
	
	
	
	
}
