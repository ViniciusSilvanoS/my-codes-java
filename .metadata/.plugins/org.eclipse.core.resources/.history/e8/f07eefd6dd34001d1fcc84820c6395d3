package classe;

public class DesafioDataTeste {
	
	public static void main(String[] args) {
		DesafioData d1 = new DesafioData();
		d1.dia = 5;
		d1.mes = 10;
		d1.ano = 1992;
		
		DesafioData d2 = new DesafioData();
		d2.dia = 15;
		d2.mes = 02;
		d2.ano = 1968;
		
		System.out.printf("1ª data: d% / d% / d%.", data1.dia, data1.mes, data1.ano);
		System.out.printf("2ª data: d% / d% / d%.", data2.dia, data2.mes, data2.ano);
	}
	
}
