package classe;

public class DesafioDataTeste {
	
	public static void main(String[] args) {
		DesafioData data1 = new DesafioData();
		data1.dia = 5;
		data1.mes = 10;
		data1.ano = 1992;
		
		DesafioData data2 = new DesafioData();
		data2.dia = 15;
		data2.mes = 02;
		data2.ano = 1968;
		
		System.out.printf("1ª data: d% / d% / d%.", data1.dia, data1.mes, data1.ano);
		System.out.printf("2ª data: d% / d% / d%.", data2.dia, data2.mes, data2.ano);
	}
	
}
