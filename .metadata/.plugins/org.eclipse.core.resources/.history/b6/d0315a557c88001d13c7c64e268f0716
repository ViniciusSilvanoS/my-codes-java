package youtube.ze_codes_projetos.mercado.model;

public class Produto {

	private static int count = 1;
	
	private int id;
	private String nome;
	private double preco;
	
	
	public Produto(String nome, double preco) {
		super();
		this.id = Produto.count;
		this.nome = nome;
		this.preco = preco;
		Produto.count += 1;
	}
	
	
}
