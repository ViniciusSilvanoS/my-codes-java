package lambdas;

import java.util.Arrays;
import java.util.List;
import java.util.function.Consumer;

public class P4Consumer {

	public static void main(String[] args) {
		
		Consumer<P3Produto> imprimir = p -> System.out.println(p.nome);
		
		P3Produto p1 = new P3Produto("Caneta", 12.34, 0.9);
		imprimir.accept(p1);
		
		P3Produto p2 = new P3Produto("Notebook", 2987.99, 0.09);
		P3Produto p3 = new P3Produto("Notebook", 19.90, 0.09);
		P3Produto p4 = new P3Produto("Notebook", 07.80, 0.09);
		P3Produto p5 = new P3Produto("Notebook", 04.39, 0.09);
		
		List<P3Produto> produtos = Arrays.asList(p1);
		
		
		
	}
	
}
