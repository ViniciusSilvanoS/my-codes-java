package classe;

public class DesafioDataTeste {
	
	public static void main(String[] args) {
		DesafioData d1 = new DesafioData();
		d1.dia = 5;
		d1.mes = 10;
		d1.ano = 1992;
		
		DesafioData d2 = new DesafioData();
		d2.dia = 15;
		d2.mes = 02;
		d2.ano = 1968;
		
		System.out.println("1ª data: %d/%d/%d.", d1.dia, d1.mes, d1.ano);
		System.out.printf("2ª data: %d/%d/%d.", d2.dia, d2.mes, d2.ano);
	}
	
}
