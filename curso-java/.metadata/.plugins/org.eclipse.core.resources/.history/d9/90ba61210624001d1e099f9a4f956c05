package fundamentos;

import java.util.Scanner;

public class Temperatura {

	public static void main(String[] args) {
		// (ºF - 32) x 5/9 = ºC
		double f = 80.0;
		double c;
		
		//Scanner f = new Scanner (System.in);
		c = (f - 32)*5/9;
		System.out.println("Temperatura em Cº é: " + c);
//		System.
		
		
	}
	
}
