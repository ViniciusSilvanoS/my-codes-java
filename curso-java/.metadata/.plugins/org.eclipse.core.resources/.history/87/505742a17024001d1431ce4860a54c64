package fundamentos;

public class AreaCircunferencia {

	public static void main(String[] args) {
		double raio = 5;
		final double PI = 3.14159;
		int circunferencia = 10;
		double area = Math.pow(raio, 2) * PI;
		
		
		System.out.println("Área do circulo = " + area);
		
		
		raio = 10;
		area = Math.pow(raio, 2) * PI;
		System.out.println("Área = " + area + "mc.");
		
	}
	
}
